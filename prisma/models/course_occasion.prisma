enum Semester {
  HT
  VT
}

model CourseOccasion {
  id              Int      @id @default(autoincrement())
  year            Int
  semester        Semester
  courseCode      String
  programCourseID Int

  periods           CourseOccasionPeriod[]
  recommendedMasters CourseOccasionRecommendedMaster[]

  Course Course @relation(fields: [courseCode, programCourseID], references: [code, programCourseID], onDelete: Cascade)
}
