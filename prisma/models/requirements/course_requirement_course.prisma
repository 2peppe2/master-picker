model CourseRequirementCourse {
  coursesRequirementId Int
  courseCode           String
  programCourseID      Int

  coursesRequirement CoursesRequirement @relation(fields: [coursesRequirementId], references: [id])
  course             Course             @relation(fields: [courseCode, programCourseID], references: [code, programCourseID])

  @@id([coursesRequirementId, courseCode, programCourseID])
}