model CourseRequirementCourse {
  coursesRequirementId Int
  courseCode           String
  programCourseID      Int

  coursesRequirement CoursesRequirement @relation(fields: [coursesRequirementId], references: [id], onDelete: Cascade)
  course             Course             @relation(fields: [courseCode, programCourseID], references: [code, programCourseID], onDelete: Cascade)

  @@id([coursesRequirementId, courseCode, programCourseID])
}
