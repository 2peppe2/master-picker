model CoursesRequirement {
  id Int @id @default(autoincrement())

  type          CoursesType
  courses       CourseRequirementCourse[]
  requirementId Int
  requirement   Requirement @relation(fields: [requirementId], references: [id], onDelete: Cascade)
}

enum CoursesType {
  COURSES_OR // Requires one of the courses in the list when applied to a course requirement.
}
