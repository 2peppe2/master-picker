model Course {
  code       String   @id
  credits    Int
  level      String
  name       String
  link       String
  examiner   String
  literature String[]
  updatedAt  DateTime @default(now())

  // Relationship mappings.
  Examination        Examination[]
  CourseMaster       CourseMaster[]
  CourseOccation     CourseOccation[]
}
