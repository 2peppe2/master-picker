model Course {
  code      String   @id
  credits   Float
  level     String
  name      String
  link      String
  examiner  String
  ecv       String
  prerequisitesText   String
  scheduledHours      Int
  selfStudyHours     Int
  updatedAt DateTime @default(now())
  programCourseID Int

  // Relationship mappings.
  Examination         Examination[]
  CourseMaster        CourseMaster[]
  CourseOccasion      CourseOccasion[]
  CoursesRequirements CoursesRequirement[]

  ProgramCourse ProgramCourse @relation (fields: [programCourseID], references: [id], onDelete: Cascade)

}
