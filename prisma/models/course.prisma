model Course {
  code      String   @id
  credits   Float
  level     String
  name      String
  link      String
  examiner  String
  ecv       String
  prerequisitesText   String
  scheduledHours      Int
  selfStudyHours     Int
  updatedAt DateTime @default(now())

  // Relationship mappings.
  Program             Program[]
  Examination         Examination[]
  CourseMaster        CourseMaster[]
  CourseOccasion      CourseOccasion[]
  CoursesRequirements CoursesRequirement[]
}
