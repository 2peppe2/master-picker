model CourseOccasionPeriod {
  id               Int @id @default(autoincrement())
  courseOccasionId Int
  period           Int

  blocks CourseOccasionBlock[]

  courseOccasion CourseOccasion @relation(fields: [courseOccasionId], references: [id], onDelete: Cascade)

  @@unique([courseOccasionId, period])
}