model CourseOccasionRecommendedMaster {
  courseOccasionId Int
  master           String
  masterProgram    String

  courseOccasion CourseOccasion @relation(fields: [courseOccasionId], references: [id], onDelete: Cascade)
  Master          Master         @relation(fields: [master, masterProgram], references: [master, masterProgram], onDelete: Cascade)

  @@id([courseOccasionId, master, masterProgram])
  @@index([master, masterProgram])
}
